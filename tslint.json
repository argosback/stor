{
    "defaultSeverity": "error",
    "extends": [
    ],
    "jsRules": {
    	"max-line-length": {
            "options": [180]
        }
    },
    "rules": {
    	"semicolon": [true, "always"],
	"quotemark": [true, "double"],
	"object-literal-sort-keys": false,
	"eofline": false,
	"ordered-imports": false,
	"no-irregular-whitespace": false,
	"no-console": [true, "error"]
    },
    "rulesDirectory": []
}
